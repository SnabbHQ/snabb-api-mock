{
  "name": "snabb-api",
  "version": "1.0.0",
  "description": "Move your app forward with the Snabb API",
  "main": "scripts/startServer.js",
  "scripts": {
    "prestart": "npm install",
    "start": "node scripts/startServer.js",
    "serve": "node_modules/.bin/nodemon -e js,yaml",
    "debug": "node_modules/.bin/nodemon debug -e js,yaml",
    "spec-gen": "node scripts/specGen.js",
    "code-gen": "npm run spec-gen && swagger-codegen generate -i swagger.json -l nodejs-server -o ."
  },
  "keywords": [
    "swagger"
  ],
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "connect": "^3.2.0",
    "yaml-js": "^0.1.3",
    "json-refs": "^2.1.6",
    "swagger-tools": "0.10.1"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "swagger-js-codegen": "^1.7.10"
  }
}
